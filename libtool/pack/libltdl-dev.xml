<?xml version="1.0" encoding="utf-8"?>
<package name="libltdl-dev" section="libdevel">
    <description>
        <summary>A system independent dlopen wrapper for GNU libtool</summary>
        <p>
        This package contains the header files and static libraries for the
        libltdl package.
        </p>
    </description>

    <requires>
        <package name="libltdl7" version="=="/>
    </requires>

    <contents>
        <file src="/usr/include/"/>
        <file src="/usr/lib/libltdl.{a,la,so}"/>
        <file src="/usr/share/libtool/libltdl/"/>
        <file src="/usr/share/aclocal/ltdl.m4"/>
    </contents>
</package>
