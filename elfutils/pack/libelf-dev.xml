<?xml version="1.0" encoding="utf-8"?>
<package name="libelf-dev" section="libdevel">
    <description>
        <summary>libelf1 development libraries and header files</summary>
        <p>
        libelf1 provides a shared library which allows reading and writing of ELF
        files on a high level.
        </p>
        <p>
        This package contains development libraries and header files for libelf1.
        </p>
    </description>

    <requires>
        <package name="libelf1" version="=="/>
    </requires>

    <contents>
        <file src="/usr/include/libelf.h"/>
        <file src="/usr/include/gelf.h"/>
        <file src="/usr/include/nlist.h"/>
        <file src="/usr/include/elfutils/elf-knowledge.h"/>
        <file src="/usr/include/elfutils/version.h"/>
        <file src="/usr/lib/libelf.{a,so}"/>
    </contents>
</package>
