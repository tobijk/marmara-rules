<?xml version="1.0" encoding="utf-8"?>
<package name="libobjc2" section="libs">
    <description>
        <summary>Runtime library for GNU Objective-C applications</summary>
        <p>
        Library needed for GNU ObjC applications linked against the shared library.
        </p>
    </description>

    <requires>
        <package name="gcc-4.5-base" version="=="/>
    </requires>

    <contents>
        <file src="/usr/lib/gcc/*/4.5/libobjc.so.*"/>
        <file src="/usr/lib/gcc/*/4.5/libobjc_gc.so.*"/>
    </contents>
</package>
